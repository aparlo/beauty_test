extends layout

block content  
    form(method="POST" action="/file_upload" encType="multipart/form-data")
        input(name="files[]" type="file" multiple="multiple")
        button(tyoe="submit") Upload
    .uk-child-width-1-3(uk-grid uk-lightbox="animation: slide")
        each val, i in Ses.user.portfolio
            div
                a.uk-inline(href=val caption=val)
                    img(src=val)